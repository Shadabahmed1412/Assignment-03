#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    double p=0,m=0,z=0;
    cin>>n;
    int a[n];
    for(int i=1;i<=n;i++) cin>>a[i];
    for(int i=1;i<=n;i++){
        if(a[i]>0) p++;
        else if(a[i]<0) m++;
        else if(a[i]==0) z++;
    }
    cout<<p/n<<endl<<m/n<<endl<<z/n<<endl;
    return 0;
}
