#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main(){

   long int a[5];
    int sum = 0;
for(int i=0;i<5;i++){
    cin>>a[i];
    sum = sum+a[i];
}
int min= a[0];
for(int i=0;i<5;i++){
if(a[i] <min){
    min = a[i];
}
}

int max =a[0];
for(int i=0;i<5;i++){
if(a[i] >max){
    max= a[i];
}
}

int smin= sum - min;
int smax = sum- max;
cout<<smax<<" "<<smin;


    return 0;
    
}
